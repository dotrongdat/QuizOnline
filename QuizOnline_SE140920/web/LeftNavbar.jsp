        <div class="sb-sidenav-footer">
            <div class="small">Logged in as:</div>
            <c:if test="${sessionScope.ACCOUNT.admin==true}">LECTURE</c:if>
            <c:if test="${sessionScope.ACCOUNT.admin==false}">STUDENT</c:if>
